@using EDziennik.Models
@model List<EDziennik.Models.StudentsViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>User Roles</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>
                <a class="btn btn-primary" asp-controller="Marks" asp-action="Index" asp-route-userId="@user.UserId">Marks</a>
            </td>
        </tr>
        }
    </tbody>
</table>